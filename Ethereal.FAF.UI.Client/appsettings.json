      "ModsPreviews": "C:/ProgramData/FAForever/cache/mod_previews/"
    },
    "UidGenerator": "External/faf-uid.exe",
    "IceAdapter": {
      "Executable": "External/faf-ice-adapter.jar",
      "Logs": "C:/ProgramData/FAForever/logs/iceAdapterLogs/ice-adapter.log",
      "ForceRelay": false
    },
    "MapGenerator": {
      "Versions": "C:/ProgramData/FAForever/map_generator/",
      "Logs": "C:/ProgramData/FAForever/logs/map_generator.log",
