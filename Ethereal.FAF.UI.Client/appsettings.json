  },
  "FAForever": {
    "Lobby": {
      "Host": "lobby.faforever.com",
      "Port": "8002"
    },
    "Replay": {
      "Host": "lobby.faforever.com",
      "Port": "15000"
    },
    "Relay": {
      "Host": "lobby.faforever.com",
      "Port": "8000"
    },
    "IRC": {
