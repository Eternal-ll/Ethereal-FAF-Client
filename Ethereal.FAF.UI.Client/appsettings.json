    "Content": "https://content.faforever.com/",
    "OAuth": {
      "BaseAddress": "https://hydra.faforever.com/oauth2/",
      "ResponseSeconds": 30,
      "ClientId": "ethereal-faf-client",
      "Scope": "openid+offline+public_profile+lobby+upload_map+upload_mod",
