{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    }
  },
  "Paths": {
    "LocalReplaysMetadata": "C:/ProgramData/FAForever/cache/local_replays_metadata/",
    "Cache": {
      "Directory": "C:/ProgramData/FAForever/cache/",
      "Avatars": "C:/ProgramData/FAForever/cache/avatars",
