    <Style x:Key="ForgedAllianceFactionPathStyle" TargetType="Path">
        <Style.Triggers>
            <DataTrigger Binding="{Binding}" Value="1">
                <Setter Property="Fill">
                    <Setter.Value>
                        <SolidColorBrush Color="{StaticResource ForgedAlliance_UEF_FactionColor}"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Data" Value="{StaticResource ForgedAlliance_UEF_LogoGeometry}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding}" Value="2">
                <Setter Property="Fill">
                    <Setter.Value>
                        <SolidColorBrush Color="{StaticResource ForgedAlliance_Aeon_FactionColor}"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Data" Value="{StaticResource ForgedAlliance_Aeon_LogoGeometry}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding}" Value="3">
                <Setter Property="Fill">
                    <Setter.Value>
                        <SolidColorBrush Color="{StaticResource ForgedAlliance_Cybran_FactionColor}"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Data" Value="{StaticResource ForgedAlliance_Cybran_LogoGeometry}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding}" Value="4">
                <Setter Property="Fill">
                    <Setter.Value>
                        <SolidColorBrush Color="{StaticResource ForgedAlliance_Seraphim_FactionColor}"/>
                    </Setter.Value>
                </Setter>
                <Setter Property="Data" Value="{StaticResource ForgedAlliance_Seraphim_LogoGeometry}"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>
</ResourceDictionary>